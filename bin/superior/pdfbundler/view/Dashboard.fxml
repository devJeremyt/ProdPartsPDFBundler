<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="475.0" minWidth="850.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="superior.pdfbundler.controller.DashboardController">
   <children>
      <Button fx:id="importButton" layoutX="90.0" layoutY="45.0" mnemonicParsing="false" onAction="#importCSV" text="Import">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <TableView fx:id="partsTable" layoutX="160.0" layoutY="132.0" minWidth="550.0" prefHeight="279.0" prefWidth="550.0">
         <columns>
            <TableColumn fx:id="partNumberColumn" minWidth="275.0" prefWidth="318.0" text="Part Number" />
            <TableColumn fx:id="drawingFoundColumn" minWidth="180.0" prefWidth="232.0" text="Drawing Found" />
         </columns>
      </TableView>
      <TextField fx:id="csvFileLocation" layoutX="211.0" layoutY="45.0" prefHeight="37.0" prefWidth="448.0">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button fx:id="exportButton" layoutX="693.0" layoutY="45.0" mnemonicParsing="false" onAction="#exportPDF" text="Export to PDF">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
